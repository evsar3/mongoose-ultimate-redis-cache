{
  "name": "mongoose-ultimate-redis-cache",
  "version": "1.0.0",
  "description": "Redis cache for mongoose",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "npx ts-node-dev ./src/index.ts",
